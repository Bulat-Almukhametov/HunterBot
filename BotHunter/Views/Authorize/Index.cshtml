
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="ru">
<head>
    <link rel="shortcut icon" type="image/x-icon" href="data:data:image/png;base64,">
    <meta charset="utf-8" />
    <title>Навикон Бот</title>
    <link rel="stylesheet" href="~/Content/Authorize.css">
    <script src="~/Scripts/sha1.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-1.10.2.js" type="text/javascript"></script>
</head>
<body>
        <div class="container">
            <section id="content">
                <form id="authorizationForm" method="post" action="~/Authorize/Login">
                    <h1>Авторизация</h1>
                @{ 
                    var bigNumber = Guid.NewGuid();
                    TempData["BigNumber"] = bigNumber;
                }
                <input type="hidden" id="BigNumber" value=@bigNumber />
                    <div>
                        <input type="text" name="Login" placeholder="Логин" required="" id="login" />
                    </div>
                    <div>
                        <input type="password" placeholder="Пароль" required="" id="password" />
                    </div>
                    <div>
                        <input type="hidden" id="HashValue" name="HashValue" />
                        <input type="submit" value="Войти" />
                        <!--a href="#">Lost your password?</a>
                        <a href="#">Register</a-->
                    </div>
                </form><!-- form -->
                <div class="button">
                    <a href="https://navicongroup.ru/career/">NAVICON CAREER</a>
                </div><!-- button -->
            </section><!-- content -->
        </div><!-- container -->
    <script type="text/javascript">
        $(function () {
            $("#authorizationForm input[type='submit']").click(function (event) {            

                $("#HashValue").val(sha1(sha1($("#password").val()) + $("#BigNumber").val()));
            });

        });
    </script>

    </body>

</html>
